        <activity android:name=".view.activity.LoginActivity" />
        <activity android:name=".view.activity.ChartActivity" />
        <activity android:name=".view.activity.Category" />
        <activity android:name=".view.activity.MonthlyStatics" />
        <activity android:name=".view.activity.ExpectedActivity" />
        <activity android:name=".view.activity.SettingActivity" />
        <activity android:name=".view.activity.AccountActivity" />
        <activity android:name=".view.activity.NewTransferActivity" />
        <activity android:name=".view.activity.CashActivity" />
        <activity android:name=".view.activity.CreditActivity" />
